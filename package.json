{
  "name": "tmlg",
  "version": "0.0.6",
  "license": "MIT",
  "preferGlobal": true,
  "description": "simple timelogging for web and cli with exports",
  "bin": {
    "tmlg": "./bin/tmlg"
  },
  "keywords": ["timelog", "work", "worklog", "timetracking"],
  "scripts": {
    "build-cli": "vite build --config vite-cli.config.ts && npm run rename-cli",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "rename-cli": "node scripts/rename.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "dependencies": {
    "http-server": "^14.1.0",
    "picocolors": "^1.0.0",
    "inquirer": "^8.2.2",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "bootstrap": "^5.1.3",
    "bootstrap-dark-5": "github:vinorodrigues/bootstrap-dark-5",
    "easyqrcodejs": "^4.4.10",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "minimist": "^1.2.6",
    "node-fetch": "^3.2.3",
    "pinia": "^2.0.12",
    "prettier": "^2.5.1",
    "typescript": "~4.5.5",
    "ua-parser-js": "^1.0.2",
    "vite": "^2.8.4",
    "vue": "^3.2.31",
    "vue-tsc": "^0.31.4",
    "xlsx": "^0.18.4"
  }
}
